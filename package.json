{
	"name": "rename-cli",
	"version": "1.0.0",
	"description": "Rename files and folders in root folder",
	"author": "yuran1811",
	"license": "MIT",
	"bin": "src/index.js",
	"main": "src/index.js",
	"type": "module",
	"keywords": [
		"cli",
		"rename"
	],
	"scripts": {
		"pkg": "pkg -o cli -t node16-linux,node16-macos,node16-win -C GZip --public-packages \"*\" --public ./build/src/index.js",
		"clean": "rm -rf build && mkdir build",
		"build-babel": "babel ./src -d ./build/src",
		"build": "npm run clean && npm run build-babel"
	},
	"dependencies": {
		"@babel/runtime": "^7.18.3",
		"commander": "^9.3.0",
		"inquirer": "^8.2.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.18.2",
		"@babel/node": "^7.17.10",
		"@babel/plugin-transform-runtime": "^7.18.2",
		"@babel/preset-env": "^7.18.2"
	}
}
